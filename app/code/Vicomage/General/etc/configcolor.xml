<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <vicomage_color_config>
            <general_config>
                <config_as_developer>1</config_as_developer>
            </general_config>
            <config_color_base>
                <enable_base_color>1</enable_base_color>
                <base_color>1</base_color>
            </config_color_base>
            <config_color_header>
                <enable_header_color>1</enable_header_color>
                <header_color>1</header_color>
            </config_color_header>
            <config_color_content>
                <enable_content_color>1</enable_content_color>
                <content_color>1</content_color>
            </config_color_content>
            <config_color_footer>
                <enable_footer_color>1</enable_footer_color>
                <footer_color>1</footer_color>
            </config_color_footer>
        </vicomage_color_config>
    </default>
</config>
